{
  "name": "code-insight-analyst",
  "version": "0.1.0",
  "description": "高性能、多功能的代码分析工具",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "code-insight": "bin/code-insight.js"
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "lint": "eslint src --max-warnings=150",
    "format": "prettier --write .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "code-analysis",
    "code-coverage",
    "duplicate-code",
    "unused-code"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.29.0",
    "@types/commander": "^2.12.2",
    "@types/ejs": "^3.1.5",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.3",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.2",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "ejs": "^3.1.10",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "fs-extra": "^11.1.1",
    "globals": "^16.2.0",
    "jest": "^29.6.1",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsup": "^7.1.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "cli-table3": "^0.6.3",
    "commander": "^11.0.0",
    "console-table-printer": "^2.14.3",
    "inquirer": "^8.2.5",
    "listr2": "^6.6.0",
    "nyc": "^15.1.0",
    "ora": "^5.4.1",
    "ts-morph": "^19.0.0"
  }
}
